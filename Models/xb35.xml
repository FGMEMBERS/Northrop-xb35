  <!-- Animations des hélices -->
  <!-- Sélection/déselection en fonction des rpm moteurs -->
  <!-- Moteur 1 extérieur gauche -->
  <animation>
    <type>select</type>
    <object-name>propdiscGE1</object-name>
    <object-name>propdiscGE2</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurGE1</object-name>
    <object-name>propblurGE2</object-name>
    <condition>
    <and>
