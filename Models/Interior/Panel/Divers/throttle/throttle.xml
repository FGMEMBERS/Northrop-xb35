<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <object-name>moteur1</object-name>
    <object-name>moteur2</object-name>
    <object-name>moteur3</object-name>
    <object-name>moteur4</object-name>
  </animation>
     
  <!-- Manette moteur N째1 -->
  <animation>
    <type>rotate</type>
    <object-name>moteur1</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep> -99 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.017 </x-m>
      <y-m> -0.112 </y-m>
      <z-m> -0.180 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Manette moteur N째2 -->
