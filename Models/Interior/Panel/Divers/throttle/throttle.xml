<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>throttle.ac</path>

  <!-- Manette moteur N°1 -->
  <animation>
    <type>rotate</type>
    <object-name>moteur1</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry><ind>  0.0 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  1.0 </ind><dep> -99.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.017 </x-m>
      <y-m> -0.112 </y-m>
      <z-m> -0.180 </z-m>
    </center>
    <axis>
      <x> 0.0 </x>
      <y> 1.0 </y>
      <z> 0.0 </z>
    </axis>
  </animation>

